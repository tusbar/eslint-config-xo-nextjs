{
  "name": "eslint-config-xo-nextjs",
  "version": "8.0.0",
  "description": "ESLint shareable config for Next.js to be used with eslint-config-xo",
  "repository": "https://github.com/tusbar/eslint-config-xo-nextjs",
  "author": "Bertrand Marron",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./space": {
      "import": "./dist/space.js",
      "types": "./dist/space.d.ts"
    }
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "check:ts": "tsc --noEmit",
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "keywords": [
    "react",
    "jsx",
    "eslintconfig",
    "xo",
    "xoxo",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "jscs",
    "jshint",
    "jslint",
    "eslint",
    "validate",
    "code style",
    "standard",
    "strict",
    "check",
    "checker",
    "verify",
    "enforce",
    "hint",
    "simple",
    "vercel",
    "nextjs"
  ],
  "dependencies": {
    "eslint-config-next": "^16.1.1",
    "eslint-config-xo-react": "^0.29.0"
  },
  "devDependencies": {
    "@bizon/semantic-release-config": "^3.1.0",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@tsconfig/node24": "^24.0.4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.7",
    "eslint-config-xo-bizon": "^4.2.0",
    "jest": "^30.2.0",
    "next": "^16.1.1",
    "react": "^19.2.3",
    "semantic-release": "^25.0.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "xo": "^1.2.3"
  },
  "release": {
    "extends": "@bizon/semantic-release-config"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/space.ts"
    ],
    "format": [
      "esm"
    ],
    "dts": true
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
